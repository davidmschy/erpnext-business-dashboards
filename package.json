{
  "name": "erpnext-business-dashboards",
  "version": "1.0.0",
  "description": "Live operational dashboards and business websites integrated with ERPNext",
  "private": true,
  "workspaces": [
    "dashboards/*",
    "websites/*",
    "shared"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "clean": "turbo run clean",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "prettier": "^3.1.0",
    "turbo": "^1.11.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/davidmschy/erpnext-business-dashboards.git"
  },
  "author": "Genii Operations Team",
  "license": "MIT"
}