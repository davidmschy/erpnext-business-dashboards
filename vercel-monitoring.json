{
  "buildCommand": "cd dashboards/monitoring && npm install && npm run build",
  "outputDirectory": "dashboards/monitoring/.next",
  "installCommand": "cd dashboards/monitoring && npm install",
  "framework": "nextjs",
  "regions": ["iad1"],
  "env": {
    "NEXT_PUBLIC_ERPNEXT_URL": "@erpnext-url",
    "NEXT_PUBLIC_ERPNEXT_API_KEY": "@erpnext-api-key",
    "NEXT_PUBLIC_ERPNEXT_API_SECRET": "@erpnext-api-secret",
    "NEXT_PUBLIC_REFRESH_INTERVAL": "30000"
  }
}